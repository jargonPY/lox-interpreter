fun sayHi(first, last) {
  print "Hi, " + first + " " + last + "!";
}

sayHi("Dear", "Reader");




fun sayHiReturn(first, last) {
  return "Returning: " + "Hi, " + first + " " + last + "!";
}

var value = sayHiReturn("Dear", "Reader");
print value;





fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 10; i = i) {
  print fib(i);
  i = i + 1;
}